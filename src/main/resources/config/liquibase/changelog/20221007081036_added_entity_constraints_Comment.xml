<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Comment.
    -->
    <changeSet id="20221007081036-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="copost_id_obj_id"
                                 baseTableName="comment"
                                 constraintName="fk_comment__copost_id_obj_id"
                                 referencedColumnNames="id"
                                 referencedTableName="post"/>

        <addForeignKeyConstraint baseColumnNames="added_by_user_id"
                                 baseTableName="comment"
                                 constraintName="fk_comment__added_by_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usermaster"/>

        <addForeignKeyConstraint baseColumnNames="updated_by_user_id"
                                 baseTableName="comment"
                                 constraintName="fk_comment__updated_by_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usermaster"/>

        <addForeignKeyConstraint baseColumnNames="approved_by_user_id"
                                 baseTableName="comment"
                                 constraintName="fk_comment__approved_by_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usermaster"/>

        <addForeignKeyConstraint baseColumnNames="org_id_obj_id"
                                 baseTableName="comment"
                                 constraintName="fk_comment__org_id_obj_id"
                                 referencedColumnNames="id"
                                 referencedTableName="organisation"/>
    </changeSet>
</databaseChangeLog>
